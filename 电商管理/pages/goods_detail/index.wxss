page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .cates_container {
  display: flex;
  height: calc( 100vh - 90rpx );
}
.cates .cates_container .left_menu {
  flex: 2;
}
.cates .cates_container .left_menu .menu-item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cates .cates_container .right_menu {
  flex: 5;
  background-color: blue;
}
